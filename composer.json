{
    "name": "hinet/laravel-summernote",
    "description": "Summernote editor integration for Laravel.",
    "license": "MIT",
    "keywords": ["summernote", "laravel"],
    "authors": [
        {
            "name": "overtrue",
            "email": "anzhengchao@gmail.com"
        },
        {
            "name": "hinet",
            "email": "hinet@live.cn"
        }
    ],
    "autoload": {
        "psr-4":{
            "Hinet\\LaravelSummernote\\":"src/"
        }
    },
    "suggest": {
        "overtrue/laravel-filesystem-qiniu": "如果你想要使用七牛云存储，也许你需要安装它哦~"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hinet\\LaravelSummernote\\SummernoteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
